<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    
    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">

    <!-- Google Fonts -->
    <link href='https://fonts.googleapis.com/css?family=Oswald|Open+Sans:400,400italic,600italic,600' rel='stylesheet' type='text/css'>
    
    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title || "Raisa Yang - Front end developer" %></title>
    
    <%= stylesheet_link_tag "main" %>
    <%= javascript_include_tag  "all" %>
  </head>
  
  <body class="<%= page_classes %>">
    <%= yield %>
  </body>
</html>